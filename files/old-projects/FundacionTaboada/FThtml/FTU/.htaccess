IndexOptions FancyIndexing
Options All
RewriteEngine on
# RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /([^/]+/)*[^.]+\.php
# RewriteRule ^(([^/]+/)*[^.]+)\.php$ http://www.fundaciontaboada.com/$1 [R=301,L]
# RewriteCond %{REQUEST_FILENAME}.php -f
# RewriteRule ^(([^/]+/)*[^/.]+)$ /$1.php [L]

RewriteRule ^Inicio$ Inicio.php [L]
RewriteRule ^Ayuda$ Ayuda.php [L]
RewriteRule ^Becas$ Becas.php [L]
RewriteRule ^Contacto$ Contacto.php [L]
