RewriteRule ^en/Becas$ en/Becas.php [L]
RewriteRule ^en/Contacto$ en/Contacto.php [L]
RewriteRule ^en/Fotografias$ en/Fotografias.php [L]
RewriteRule ^en/Historia$ en/Historia.php [L]
RewriteRule ^en/Mision$ en/Mision.php [L]
RewriteRule ^en/Noticias$ en/Noticias.php [L]
RewriteRule ^en/Preguntas$ en/Preguntas.php [L]
